<dsui>
output_file = "/tmp/file-locking.dsui.bin"


<dsui_enabled>
FUNC_MAIN = [
	CREATE_THREAD_PRODUCER
	CREATE_THREAD_APPENDER
	CREATE_THREAD_CONSUMER
	CLOSING_ALL_THREADS
	MAIN_THREAD
]
PRODUCER_THREAD = [
	EXIT
	WRITTEN_TO_FILE
	ENTER
]
CONSUMER_THREAD = [
	EXIT
	READ_FROM_FILE
	ENTER
]	
APPENDER_THREAD = [
	EXIT
	WRITTEN_TO_FILE
	ENTER
]

